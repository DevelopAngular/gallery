(function($) {
    $("form[data-form=category_list]").submit(function(event) {
        var $this = $(this),
            $inputs = $("input[name='catIds[]']", $this),
            subcatIds = $inputs.map(function(index, element) {
                return $(this).val();
            }).get().join();
        $inputs.remove();
        if (subcatIds.length) {
            $this.prop("action", $this.prop("action") + "/" + subcatIds);
        }
    });
})(jQuery);


