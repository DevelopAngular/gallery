(
function ($, modalId, formId) {
    var ReportNewsHelper = function (modalId, formId) {
        var photos = [],
            timer = null;
        ReportNewsHelper.prototype.beginAutoCloseTimer = function () {
            if (null !== timer) {
                clearTimeout(timer);
            }
            timer = setTimeout(function () {
                $(".modal-inner-alert", "#" + modalId).slideUp();
                timer = null;
            }, 10000);
        };

        $("#" + modalId)
            .on("click", ".js-browse-button", function () {
                $(this).nextAll(":file").click();
            })
            .on("click", ".js-file-uploader .js-clear-button", function () {
                $(this).prevAll(":file").val(null).trigger("change");
            })
            .on("change", ".js-file-uploader :file", function () {
                var isFileSelected = 0 !== this.files.length;
                $(this).prevAll(".js-browse-button").toggle(!isFileSelected);
                $(this).nextAll(".js-file-name").toggle(isFileSelected);
                $(this).nextAll(".js-clear-button").toggle(isFileSelected);
                if (isFileSelected) {
                    $(this).nextAll(".js-file-name").text(this.files[0].name);
                }
            })
            .on("click", ".js-photos-uploader .js-clear-button", function () {
                var $this = $(this),
                    $wrapper = $this.closest(".custom-uploader"),
                    $itemWrapper = $this.closest(".js-file-item"),
                    itemIndex = $wrapper.find(".js-file-item:visible").index($itemWrapper);
                if ("undefined" !== typeof itemIndex && itemIndex < photos.length) {
                    photos.splice(itemIndex, 1);
                }
                $itemWrapper.remove();
            })
            .on("change", ".js-photos-uploader :file", function () {
                var $this = $(this),
                    $sampleWrapper = $this.closest(".custom-uploader").find(".js-file-item:hidden");
                    isFileSelected = 0 !== this.files.length;
                if (isFileSelected) {
                    for (var i = 0; i < this.files.length; i++) {
                        var file = this.files[i],
                            $wrapper = $sampleWrapper.clone(); 
                        photos.push(file);
                        $wrapper
                            .data({fileIndex: i})
                            .find(".js-file-name")
                                .text(file.name);
    
                        $this.closest(".custom-uploader").append($wrapper);
                        $wrapper.show();
                    }
                    $this.val(null);
                }
            })
            .on("click", ".modal-inner-alert .close", function () {
                $(this).closest(".modal-inner-alert").slideUp();
            })
            .on("hidden.bs.modal", function () {
                $(this).find(".modal-inner-alert").hide();
            })
            .on("submit", ".inside-form", function () {
                if (null !== timer) {
                    clearTimeout(timer);
                }
                $(".modal-inner-alert", "#" + modalId).slideUp();
            })
            .on("pjax:beforeSend", function (event, xhr, settings) {
                var formData = settings.data;
                formData.delete("NewsAddForm[photos][]");
                $.each(photos, function () {
                    formData.append("NewsAddForm[photos][]", this, this.name);
                });
                photos = [];
            });
    };
    window.reportNewsHelper = new ReportNewsHelper(modalId, formId);
})(jQuery, "modal-anonymous", "w86");
(
        function ($, widgetId, dateSelector, tempSelector) {
            var $widget = $("#" + widgetId),
                $list = $(".js-mini-dropdown-list", $widget),
                maxDateWidth = Math.max.apply(null, $(dateSelector, $list).map(function (index, element) {
                    return $(element).outerWidth(true);
                }).get()),
                maxTempWidth = Math.max.apply(null, $(tempSelector, $list).map(function (index, element) {
                    return $(element).outerWidth(true);
                }).get());
            $(dateSelector, $widget).outerWidth(maxDateWidth);
            $(tempSelector, $widget).outerWidth(maxTempWidth);
        })(jQuery, "w87", ".c-weather__date", ".c-weather__temp");
(function ($) {
    var $window = $(window),
        $container = $(".js-page-content"), 
        sections = $(".row[data-key]", $container).map(function () {
            var $this = $(this);

            return {
                top: $this.offset().top,
                $section: $this
            };
        }).get(),
        initialSectionsCount = $("[class*=js-content-block-]", $container).length,
        headerHeight = null,
        columnWidth = null,
        isMobileResolution = null,
        actualSectionIndex = null;
    sections.sort(function (a, b) {
        return a.top - b.top;
    });
    if (sections.length > 0) {
        sections[0].top = 0;
    }

    function getViewportTopCoord() {
        return $window.scrollTop() + headerHeight;
    }

    function getVisibleSectionIndex() {
        var viewportTopCoord = getViewportTopCoord(),
            visibleSectionIndex = sections.findIndex(function (section) {
                return viewportTopCoord < section.top;
            });
        if (-1 === visibleSectionIndex) {
            visibleSectionIndex = sections.length;
        }
        if (visibleSectionIndex > 0) {
            visibleSectionIndex -= 1;
        }

        return visibleSectionIndex;
    }

    function updateSectionTopCoords() {
        $.each(sections, function (index, element) {
            if (0 === index) {
                return;
            }
            element.top = element.$section.offset().top;
        });
    }

    $(window).on("resize", function () {
        /** @TODO: расчитывать динамически при перестройке меню + 20px */
        headerHeight = $("header").outerHeight(true) + 20;
        columnWidth = $(".js-sticky-element", $container).width();
        var isActualResolutionMobile = $window.width() < 920,
            $rightBlocks = $(".js-right-block-content", $container);
        if (!isActualResolutionMobile) {
            $rightBlocks.css({top: headerHeight, width: columnWidth});
            updateSectionTopCoords();
        }
        if (isMobileResolution !== isActualResolutionMobile) {
            var blockStyle = {position: isActualResolutionMobile ? "static" : "fixed"};
            if (isActualResolutionMobile) {
                blockStyle.width = "100%";
            }
            $rightBlocks.css(blockStyle);
            updateSectionTopCoords();
            isMobileResolution = isActualResolutionMobile;
            if (!isMobileResolution) {
                $window.triggerHandler("fastScroll");
            }
        }
    }).triggerHandler("resize");

    if (sections.length > 0 && initialSectionsCount > 0) {
        $window.on("fastScroll", function () {
            if (isMobileResolution) {
                return;
            }
            var visibleSectionIndex = getVisibleSectionIndex(),
                $section = null;
            if (actualSectionIndex !== visibleSectionIndex) {
                $section = sections[visibleSectionIndex].$section;
                var rightBlockSelector = ".js-content-block-" + (visibleSectionIndex % initialSectionsCount),
                    $rightBlock = $section.find(rightBlockSelector),
                    $rightBlockContainer = $section.find(".js-sticky-element");
                if ($rightBlockContainer.length > 0 && 0 === $rightBlock.length) {
                    $rightBlock = $(rightBlockSelector, $container);
                    if ($rightBlock.length > 0) {
                        $rightBlockContainer.empty();
                        $rightBlockContainer.append($rightBlock);
                    }
                }
                var $sectionsToHide = $(".js-right-block-content", $container); 
                if ($rightBlock.length > 0) {
                    $sectionsToHide = $sectionsToHide.not($rightBlock[0]);
                }
                $sectionsToHide.hide();
                $rightBlock.fadeIn(400, function () {
                    $window.trigger("requestReinit");
                });
                actualSectionIndex = visibleSectionIndex;
            }
            if (0 === actualSectionIndex % initialSectionsCount) {
                $section = sections[actualSectionIndex].$section;
                $section.find(".js-right-block-content")
                    .css({
                        position: $section.offset().top < getViewportTopCoord() ? "fixed" : "static"
                    });
            }
        }).triggerHandler("fastScroll");
    }

    $container.on("newPageDataLoaded", ".row[data-key]", function (event, $loadedData) {
        var $loaderButton = $(this).find("a.loader");
        if ($loaderButton.length > 0) {
            $loaderButton = $loaderButton.closest(".col-xs-12");
            var $row = $("<div>", {
                    "class": "row",
                    "attr": {"data-key": sections.length},
                    "data": {key: sections.length}
                }),
                $leftColumn = $("<div>", {"class": "col-xs-12 col-md-8 col-lg-9"}).appendTo($row),
                $rightColumn = $("<div>", {"class": "col-xs-12 col-md-4 col-lg-3 js-sticky-element"})
                    .appendTo($row);
            $("<div>", {"class": "row", "css": {margin: "0 0 20px"}}).append($loaderButton).appendTo($leftColumn);
            $("<div>", {"class": "js-right-block-content", "css": {display: "none"}}).appendTo($rightColumn);
            $container.append($row);
            sections.push({
                top: $row.offset().top,
                $section: $row
            });
            $window.triggerHandler("fastScroll");
        }
    });
})(jQuery);
