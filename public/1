<script>jQuery(function ($) {
(
    function ($) {
        $("form[data-form=rubrics-list]").submit(function (event) {
            var $this = $(this),
                $inputs = $("input[name='rubricIds[]']", $this),
                rubricIds = $inputs.map(function (index, element) {
                    return $(this).val();
                }).get().join();
            $inputs.remove();
            if (rubricIds.length) {
                var formAction = $this.prop("action");
                formAction += "/cat/" + rubricIds;
                $this.prop("action", formAction);
            }
        });
    })(jQuery);
