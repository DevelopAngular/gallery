sages, {"message":"Необходимо заполнить «Пароль»."});yii.validation.string(value, messages, {"message":"Значение «Пароль» должно быть строкой.","min":6,"tooShort":"Значение «Пароль» должно содержать минимум 6 символов.","skipOnEmpty":1});value = yii.validation.trim($form, attribute, []);}}], {"validationUrl":"\/auth\/signup-validate"});
