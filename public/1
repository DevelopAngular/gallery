jQuery("#news-list").itemsListArranger({"itemsSelector":".card-wrapper","singleBlockSelector":"div.col-sm-6.col-lg-4","doubleBlockSelector":"div.col-lg-8","mustBeDoubleSelector":".card-wrapper.configured-double","arrangeOnInit":false,"doSingleBlock":function ($block) {
            var $blockImageElement = $(".card .card__img-wrap", $block),  
                actualImage = $blockImageElement.css("backgroundImage"),
                newImage = $blockImageElement.data("swapImage");
            if (typeof newImage !== "undefined") {
                $blockImageElement
                    .css("backgroundImage", newImage)
                    .data("swapImage", actualImage);
            }
            $block.removeClass("col-lg-8").addClass("col-sm-6 col-lg-4");
        },"doDoubleBlock":function ($block) {
            var $blockImageElement = $(".card .card__img-wrap", $block),  
                actualImage = $blockImageElement.css("backgroundImage"),
                newImage = $blockImageElement.data("swapImage");
            if (typeof newImage !== "undefined") {
                $blockImageElement
                    .css("backgroundImage", newImage)
                    .data("swapImage", actualImage);
            }
            $block.removeClass("col-sm-6 col-lg-4").addClass("col-lg-8");
        },"afterListArrange":
            function (newColumnCount) {
                this.$container.stickyBanners("stuckBanners");
